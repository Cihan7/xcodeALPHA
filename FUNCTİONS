CHECK()
CMENU = 1
end
end
iBug = -1
end

function CHECK2()
gg.clearResults()
gg.setRanges(gg.REGION_C_BSS)
gg.searchNumber("22;128::29", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineAddress("368", -1, gg.TYPE_DWORD, gg.SIGN_EQUAL, 0, -1)
if gg.getResultCount() < 1 then
    LOBBYAR()
   else
   GAMEAR()
  end
end

function CHECK()
gg.clearResults()
gg.setRanges(gg.REGION_C_BSS)
gg.searchNumber("22;128::29", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineAddress("368", -1, gg.TYPE_DWORD, gg.SIGN_EQUAL, 0, -1)
if gg.getResultCount() < 1 then
    LOBBY()
    CLOBBY = 1
   else
   GAME()
   CGAME = 2
  end
end
