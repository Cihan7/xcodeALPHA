function A()
if no100 == off then 
gg.clearResults()
gg.setRanges(gg.REGION_C_BSS)
gg.searchNumber("22;128::29", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.clearResults()
smallCross.backUp[1] = gg.getValues({[1] = {address = gg.getRangesList('libUE4.so')[1].start + 28717024, flags = 16}})
    libBase(28717024, 16, 0, '➕sᴍᴀʟʟ ᴄʀᴏss ʜᴀɪʀ')
for num, offset in ipairs({19975152, 29064308, 57142404}) do
      noRecoil.backUp[num] = gg.getValues({[1] = {address = gg.getRangesList('libUE4.so')[1].start + offset, flags = 4}})
      libBase(offset, 4, 0, '🎯ɴᴏ ʀᴇᴄᴏɪʟ 100%')
    end
no100 = on
karar = 1
else
for num, value in ipairs(noRecoil.backUp) do
      gg.setValues(value)
    end
    for num, value in ipairs(smallCross.backUp) do
      gg.setValues(value)
    end
no100 = off
end
end

