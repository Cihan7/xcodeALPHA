function CLOSE()
  gg.alert("🖤 𝖠 𝖫 𝖯 𝖧 𝖠  𝖧 𝖠 𝖢 𝖪  𝖳 𝖱 🖤\n        ⚜️𝖡𝖠𝖭𝖲𝖨𝖹 𝖸𝖠𝖱𝖦𝖨 𝖣𝖠𝖦̆𝖨𝖳⚜️")
  os.exit()
end
