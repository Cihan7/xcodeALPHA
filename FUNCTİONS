gg.clearResults()
function PS() end
function setvalue(address,flags,value) PS('修改地址数值(地址,数值类型,要修改的值)') local tt={} tt[1]={} tt[1].address=address tt[1].flags=flags tt[1].value=value gg.setValues(tt) end
so=gg.getRangesList('libUE4.so')[1].start
py=0x1078FBC
setvalue(so+py,4,0)
gg.sleep(500)
py=0x10795B4
setvalue(so+py,4,0)
gg.sleep(500)
py=0x10795D4
setvalue(so+py,4,0)
gg.sleep(500)
py=0x10A315C
setvalue(so+py,4,0)
gg.sleep(500)
py=0x1079778
setvalue(so+py,4,0)
gg.sleep(600)
end

function ANTBOX()
gg.clearResults()
Name = "antenabox" 
gg.setRanges(131072)
local dataType = 16
local search = {
{14.79005432129, 0},
{19.48537445068, -12},

}
local modify = {{99999, 0},{99999, 804},{99999, 1056},{99999, 1284},}
SearchWrite(search,modify,dataType,Name)
gg.clearResults()
end

function LESS()
gg.clearResults()
gg.setRanges(8)
local tb1 = {{-298841599, 0}, {-308864511, -8}, } 
local tb2 = {{0, 0}, } 
local dt = 4
SearchWrite(tb1,tb2, dt) 
gg.clearResults()
gg.setRanges(8)
local tb1 = {{-298841599, 0}, {-309061066, -12}, } 
local tb2 = {{0, 0}, } 
local dt = 4
SearchWrite(tb1,tb2, dt) 
gg.clearResults()
--
Name = "antiShake"
gg.setRanges(16384)
local dataType = 4
local tb1 = {{-405405696, 0}, {-499122175, 4}, {-481296380, 8}, }
local tb2 = {{0, 0}, }
SearchWrite(tb1, tb2, dataType)
gg.setRanges(16384)
local dataType = 16
local tb1 = {{-1.114450155758339E28, 0}, {-2.8111605430681328E28, -12}, {-1476732160.0, -8}, {-3.7444097189855772E28, -4}, {128.0, 4}, }
local tb2 = {{0, 0}, }
SearchWrite(tb1, tb2, dataType)
gg.clearResults()
end

function SHAKE()
gg.clearResults()
gg.setRanges(32)
gg.searchNumber("0.2~0.3;53;30;1::", 16, false, 536870912, 0, -1)
gg.searchNumber("0.2~0.3;1::", 16, false, 536870912, 0, -1)
gg.getResults(1000)
gg.editAll("0", 16)
gg.clearResults()
end

function MAGIC()
  gg.clearResults()
  gg.setRanges(32)
  gg.searchNumber('69.5;35;33', 16, false, 536870912, 0, -1)
  gg.getResults(6)
  gg.editAll('120', 16)
  gg.clearResults()
--
  gg.setRanges(gg.REGION_ANONYMOUS)
  gg.searchNumber("25;30.5", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
  gg.getResults(100)
  gg.editAll("100", gg.TYPE_FLOAT)
  gg.clearResults()
  gg.searchNumber("1F;-8.6457681e12F;15F;28F;16F;26F;8F;18F:512", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber("15.0F;28.4F;16.0F;26.0F;8.4F;18.4F:512", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(500)
gg.editAll("95", gg.TYPE_FLOAT)
gg.clearResults()
end

function STAND()
MNSTAND = gg.alert("ᴀᴄ̧ ᴠᴇʏᴀ ᴋᴀᴘᴀᴛ", "[ᗩᑕ̧]", " [Kᗩᑭᗩ]")
  if MNSTAND == 1 then STANDON() end
  if MNSTAND == 2 then STANDOFF() end
end
function STANDON()
gg.clearResults()
gg.setRanges(gg.REGION_ANONYMOUS)
gg.setVisible(false)
gg.searchNumber("4138667321167981973", gg.TYPE_QWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("4138667321167981973", gg.TYPE_QWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("4138667321167981973", gg.TYPE_QWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(200)
gg.editAll("4848124999984742400", gg.TYPE_QWORD)
gg.clearResults()
end
function STANDOFF()
gg.clearResults()
gg.setRanges(gg.REGION_ANONYMOUS)
gg.setVisible(false)
gg.searchNumber("4848124999984742400", gg.TYPE_QWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("4848124999984742400", gg.TYPE_QWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineNumber("4848124999984742400", gg.TYPE_QWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(200)
gg.editAll("4138667321167981973", gg.TYPE_QWORD)
gg.clearResults()
end

