function SD675()
W3 = gg.multiChoice({
"🌀  ᴡᴀʟʟʜᴀᴄᴋ   🌀","🌀  ᴡᴀʟʟʜᴀᴄᴋ² 🌀","💛   sᴀʀɪ      💛","💚    ʏᴇşɪʟ       💚","❤       ᴋɪʀᴍɪᴢɪ         ❤ ","💜      ᴘᴇᴍʙᴇ         ??","⬛   sɪʏᴀʜ ɢᴏᴋʏᴜ̈ᴢᴜ̈  ⬛ ","🧡 ᴛᴜʀᴜɴᴄᴜ Gᴏᴋʏᴜᴢᴜ 🧡  ","⫷ ɢᴇʀɪ ɢɪᴛ ⫸"
}, nil, "🌺ᴡᴀʟʟʜᴀᴄᴋ+ʀᴇɴᴋ[675]🌺")
if W3 == nil then
else
if W3[1] == true then
WH_675()
end
if W3[2] == true then
WH2_675()
end
if W3[3] == true then
Yellow_675()
end
if W3[4] == true then
GREEN_675()
end
if W3[5] == true then
Red_675()
end
if W3[6] == true then
PINK_675()
end
if W3[7] == true then
BLACKSKY_675()
end
if W3[8] == true then
ORANGESKY_675()
end
if W3[9] == true then
WHSNAP()
end
end
PUBGMH = -1
end
