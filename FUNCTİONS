end


while true do
  if gg.isVisible(true) then
    iBug = 1
    gg.setVisible(false)
  end
  gg.clearResults()
  if iBug == 1 and CMENU == 0 then
    CHOME()
  end
  if iBug == 1 and CMENU == 1 then
    CHECK()
  end
  if iBug == 1 and CMENU == 2 then
    CHECK2()
  end
  if karar == 1 and CGAME == 2 then
  RESETLOBBY()
  CGAME = 0
  end
  if karar == 1 and CLOBBY == 1 then
  RESETGAME()
  CLOBBY = 0
end
