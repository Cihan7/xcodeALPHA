blackBody = {state = off, backUp = {}}
function blackbody()
if black == off then
blackBody.backUp[1] = gg.getValues({[1] = {address = gg.getRangesList('libUE4.so')[1].start + 46913416, flags = 4}})
    libBase(46913416, 16, 14, '⚫ 『 ʙʟᴀᴄᴋ ᴄᴏʟᴏʀ ᴀʟʟ ᴅᴇᴠɪᴄᴇs 』')
 black = on
 else
for num, value in ipairs(blackBody.backUp) do
      gg.setValues(value)
    end
black = off
end
end

