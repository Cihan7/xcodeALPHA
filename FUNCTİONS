function A1()
if true then
    local org = gg.searchNumber
    local hook = function(...)
        gg.setVisible(false)
        local ret = org(...)
        if gg.isVisible() then
            while true do os.exit() gg.clearResults() gg.clearList() end
        end
        return ret
    end
    gg.searchNumber = hook
end
gg.clearResults() 
gg.setRanges(gg.REGION_CODE_APP) 
gg.searchNumber("13,073.3740234375", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1) 
gg.getResults(9999) 
gg.editAll("0", gg.TYPE_FLOAT) 
gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber("9.21956299e-41", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1) 
