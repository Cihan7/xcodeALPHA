function HOME()
  MN = gg.choice({
    "🇹🇷 𝙱𝚢𝚙𝚊𝚜𝚜 𝙶𝙻𝙾𝙱𝙰𝙻 𝟹𝟸 𝙱𝚒𝚝",
    "🇹🇷 𝙱𝚢𝚙𝚊𝚜𝚜 𝙺𝙾𝚁𝙴 𝟹𝟸 𝙱𝚒𝚝",
    "📂 ᴅɪɢ̆ᴇʀʟᴇʀɪ",
    "🔚𝘊̧ 𝘐 𝘒 𝘐 𝘚̧"
  },nil, (os.date("【🖤 𝖠 𝖫 𝖯 𝖧 𝖠  𝖧 𝖠 𝖢 𝖪  𝖳 𝖱 🖤】\n【⚜️ Script Version 6.6⚜️】\n  %Y/%m/%d  : %H:%M:%S")))
  if MN == nil then
  else
    if MN == 1 then BYGL() end
    if MN == 2 then BYKR() end
    if MN == 3 then TEST() end
    if MN == 4 then CLOSE() end
  end
  PUBGMH = -1
end

function BYGL()
MNGLBY = gg.choice({
    " 𝐁𝐲𝐩𝐚𝐬𝐬 𝐋𝐨𝐛𝐛𝐲 V1",
    " 𝐁𝐲𝐩𝐚𝐬𝐬 𝐋𝐨𝐛𝐛𝐲 V2",
    " 𝐀𝐧𝐭𝐢 𝐑𝐞𝐩𝐨𝐫𝐭 [Lobby]",
    "[🔙G E R İ  G İ T]"
  }, nil, "【⭕ Bypass Global Menüsü】")
  if MNGLBY == nil then
  else
    if MNGLBY == 1 then KGMLG() NOTICELG() end
    if MNGLBY == 2 then KGMLB() NOTICELB() end
    if MNGLBY == 3 then KGMRPT() NOTICERPT() end
    if MNGLBY == 4 then HOME() end
  end
  PUBGMH = -1
end

