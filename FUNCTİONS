HOME = 1
BYPDONE = 0
CMENU = 0
karar = 0
function libBase(offset, type, value, name) gg.setValues({[1] = {address = gg.getRangesList('libUE4.so')[1].start + offset, flags = type, value = value}}) gg.toast(name .. ' تم تفعيل') end
function split(szFullString, szSeparator)
	local nFindStartIndex = 1
	local nSplitIndex = 1
	local nSplitArray = {}
	while true do
		local nFindLastIndex = string.find(
			szFullString,
			szSeparator,
			nFindStartIndex
		)
		if not nFindLastIndex then
			nSplitArray[nSplitIndex] = string.sub(
				szFullString,
				nFindStartIndex,
				string.len(szFullString)
			)
			break
		end
		nSplitArray[nSplitIndex] = string.sub(
			szFullString,
			nFindStartIndex,
			nFindLastIndex - 1
		)
		nFindStartIndex = nFindLastIndex + string.len(szSeparator)
		nSplitIndex = nSplitIndex + 1
	end
	return nSplitArray
end
function xgxc(szpy, qmxg)
	for x = 1, #(qmxg) do
		xgpy = szpy + qmxg[x]["offset"]
		xglx = qmxg[x]["type"]
		xgsz = qmxg[x]["value"]
		gg.setValues({[1] = {address = xgpy, flags = xglx, value = xgsz}})
		xgsl = xgsl + 1
	end
end
function xqmnb(karar)
	gg.clearResults()
	gg.setRanges(karar[1]["memory"])
	gg.searchNumber(karar[3]["value"], karar[3]["type"])
	if gg.getResultCount() == 0 then
		gg.toast(karar[2]["name"] .. "فشل التفعيل")
	else
		gg.refineNumber(karar[3]["value"], karar[3]["type"])
		gg.refineNumber(karar[3]["value"], karar[3]["type"])
		gg.refineNumber(karar[3]["value"], karar[3]["type"])
		if gg.getResultCount() == 0 then
			gg.toast(karar[2]["name"] .. "فشل التفعيل")
		else
			sl = gg.getResults(999999)
			sz = gg.getResultCount()
			xgsl = 0
			if sz > 999999 then
				sz = 999999
			end
			for i = 1, sz do
				pdsz = true
				for v = 4, #(karar) do
					if pdsz == true then
						pysz = {}
						pysz[1] = {}
						pysz[1].address = sl[i].address + karar[v]["offset"]
						pysz[1].flags = karar[v]["type"]
						szpy = gg.getValues(pysz)
						pdpd = karar[v]["lv"] .. ";" .. szpy[1].value
						szpd = split(pdpd, ";")
						tzszpd = szpd[1]
						pyszpd = szpd[2]
						if tzszpd == pyszpd then
							pdjg = true
							pdsz = true
						else
							pdjg = false
							pdsz = false
						end
					end
				end
				if pdjg == true then
					szpy = sl[i].address
					xgxc(szpy, qmxg)
					xgjg = true
				end
			end
			if xgjg == true then
				gg.toast(karar[2]["name"] .. "Modified " .. xgsl .. " Values🔍")
			else
				gg.toast(karar[2]["name"] .. "فشل تفعيل")
			end
		end
	end
end

function CHOME()
OICHOME = gg.alert("➧AʟᴘʜᴀDᴇᴠᴇʟᴏᴘᴇʀ: ʜɪʟᴇ sᴄʀɪᴘᴛ🔞\n\n➧ᴛᴇʟᴇɢʀᴀᴍ: @MᴜᴄᴏᴛᴏᴏDᴇᴠᴇʟᴏᴘᴇʀ\n➧ᴏʏᴜɴ: ᴘᴜʙɢᴍᴏʙɪʟᴇ\n➧ɢᴇʟɪşᴛɪʀɪᴄɪ: ᴍᴜᴄᴏᴛᴏᴏ|😆\n➧ʟᴜ̈ᴛғᴇɴ ɢᴀᴍᴇɢᴜᴀʀᴅɪᴀɴ ᴀʏᴀʀʟᴀʀɪɴɪ ʏᴀᴘᴍɪş ᴏʟ⚠️\n\n®2021 AʟᴘʜᴀDᴇᴠᴇʟᴏᴘᴇʀ - Oɴʟɪɴᴇ Sᴄʀɪᴘᴛ V2.0.", "⟬💥𝐘𝐚𝐫𝐠𝐢💥⟭")
if OICHOME == nil then
else
if OICHOME == 1 then
CHECK2()
CMENU = 2
end
if OICHOME == 2 then
CHECK()
CMENU = 1
end
end
iBug = -1
end

function CHECK2()
gg.clearResults()
gg.setRanges(gg.REGION_C_BSS)
gg.searchNumber("22;128::29", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineAddress("368", -1, gg.TYPE_DWORD, gg.SIGN_EQUAL, 0, -1)
if gg.getResultCount() < 1 then
    LOBBYAR()
   else
   GAMEAR()
  end
end

function CHECK()
gg.clearResults()
gg.setRanges(gg.REGION_C_BSS)
gg.searchNumber("22;128::29", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.refineAddress("368", -1, gg.TYPE_DWORD, gg.SIGN_EQUAL, 0, -1)
if gg.getResultCount() < 1 then
    LOBBY()
    CLOBBY = 1
   else
   GAME()
   CGAME = 2
  end
end
